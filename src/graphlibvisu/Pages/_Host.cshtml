@page "/"
@namespace graphlibvisu.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>graphlibvisu</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />

    <link href="vis/vis.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="alchemy/styles/vendor.css" />
    <link rel="stylesheet" href="alchemy/alchemy.css" />

</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
    <script src="vis/vis.min.js"></script>
    <script src="d3.min.js"></script>
     <script src="alchemy/scripts/vendor.js"></script>
    <script src="alchemy/alchemy.js"></script>

    <script src="graphvisu.js"></script>
</body>
</html>
